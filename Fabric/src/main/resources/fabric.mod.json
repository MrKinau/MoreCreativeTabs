{
    "schemaVersion": 1,
    "id": "morecreativetabs",
    "version": "${version}",

    "name": "MoreCreativeTabs",
    "description": "A mod that allows you to customize creative tabs. Add your own, move items to a custom tab or just disable vanilla tabs.",
    "authors": [
      "HypherionSA"
    ],
    "contact": {
      "sources": "https://github.com/hypherionmc/MoreCreativeTabs/",
      "issues": "https://github.com/hypherionmc/MoreCreativeTabs/issues"
    },

    "license": "MIT",
    "icon": "assets/modid/icon.png",
    "environment": "client",
    "entrypoints": {
      "client": [
        "me.hypherionmc.morecreativetabs.client.MoreCreativeTabsClient"
      ]
    },
    "mixins": [
      "morecreativetabs.mixins.json"
    ],

    "depends": {
      "fabricloader": ">=0.12",
      "fabric": "*",
      "minecraft": "1.18.x",
      "java": ">=17"
    }
  }
